@model IEnumerable<WebProject.Models.Report>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    if (Session["id"] == null)
    {
        Response.Redirect("~/Login/Index");
    }
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.report1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Event.eventname)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.report1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Event.eventname)
        </td>
        <td>
           
            @Html.ActionLink("Delete", "Delete", new { id=item.reportId })
        </td>
    </tr>
}

</table>
